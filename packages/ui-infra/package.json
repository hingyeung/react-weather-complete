  "main": "lib/main.js",
  "private": true,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "synth:ui:test": "npm run build && CDK_STACK_ENV=test AWS_PROFILE=samuelli-test cdk synth --app dist/main.js",
    "deploy:ui:test": "CDK_STACK_ENV=test AWS_PROFILE=samuelli-test cdk deploy --app dist/main.js",
    "destroy:ui:test": "CDK_STACK_ENV=test AWS_PROFILE=samuelli-test cdk destroy --app dist/main.js",
    "synth:api:test": "npm run build && CDK_STACK_ENV=test AWS_PROFILE=samuelli-test cdk synth --app dist/api-infra.js --no-staging",
    "deploy:api:test": "CDK_STACK_ENV=test AWS_PROFILE=samuelli-test cdk deploy --app dist/api-infra.js",
    "destroy:api:test": "CDK_STACK_ENV=test AWS_PROFILE=samuelli-test cdk destroy --app dist/api-infra.js"
  },
  "devDependencies": {
    "aws-cdk": "^1.18.0",
    "@aws-cdk/assert": "^1.18.0",
    "@types/jest": "^24.0.22",
    "@types/node": "10.17.5",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-certificatemanager": "^1.17.1",
    "@aws-cdk/aws-route53": "^1.17.1",
    "@aws-cdk/aws-route53-targets": "^1.17.1",
    "@aws-cdk/aws-s3": "^1.17.1",
    "@aws-cdk/aws-s3-deployment": "^1.17.1",
    "@aws-cdk/core": "^1.17.1",
    "@types/node": "^12.12.11",
    "dotenv": "^8.2.0",
    "source-map-support": "^0.5.16"
  },
  "_moduleAliases": {
    "@utils": "dist/packages/utils"
  }
}
